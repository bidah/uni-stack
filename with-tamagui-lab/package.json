{
  "name": "nate-fix-tamagui-expo-router",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prisma:studio": "npx prisma studio --schema src/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema src/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema src/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema src/prisma/schema.prisma",
    "prisma:introspect": "npx prisma introspect --schema src/prisma/schema.prisma"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@prisma/client": "^5.9.1",
    "@react-navigation/native": "^6.0.2",
    "@tamagui/config": "^1.90.2",
    "@tamagui/metro-plugin": "^1.90.2",
    "@tanstack/react-query": "^5.20.5",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "babel-preset-expo": "~11.0.0",
    "expo": "51.0.27",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.21",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "~2.0.4",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "^0.19.9",
    "tamagui": "latest",
    "typescript": "~5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@types/react": "~18.2.14",
    "prisma": "^5.9.1"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
